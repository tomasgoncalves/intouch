{% extends "base.html.twig" %}
{% block content %}

<!--Banner-->
<header>
    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12">
        <!-- CART BUTTON AND LANG -->
        <ul id="cart-list" class="borderBottomHeader">
            <li>
                <!--Icon Social Media on nav bar-->
                <a href="https://www.facebook.com/connectedintouchbiz/" target="_blank">
                    <div class="social-flag">
                        <img src="{{asset ('img/facebook.png')}}" alt="facebook" id="facebook">
                    </div>
                </a>
            </li>

            <li>
                <a href="https://twitter.com/IntouchBiz_" target="_blank">
                    <div class="social-flag">
                        <img src="{{asset ('img/twitter.png')}}" alt="twitter" id="twitter">
                    </div>
                </a>
            </li>
            <li>
                <a href="https://plus.google.com/u/0/111192249106908035114" target="_blank">
                    <div class="social-flag">
                        <img src="{{asset ('img/youtube.png')}}" alt="googleplus" id="googleplus">
                    </div>
                </a>
            </li>
        </ul>
    </div>  
</div>
</div>
</header>

<!-- SLIDER -->
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div id="carousel-generic" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="item active">
                        <div class="homepage-hero-module imgSlider">
                            <div class="video-container">
                                <div class="filter">
                                </div>
                                <!--Video of banner pages -->
                                <div style="background-color:rgba(33, 64, 154, 0.65);position:absolute; width:100%; height:100%;z-index:9">
                                </div>    
                                <video autoplay loop class="fillWidth">
                                    <source alt="rendimento mensal garantido" src="{{asset('video/Mock-up.mp4')}}" type="video/mp4" />Your browser does not support the video tag. I suggest you upgrade your browser.
                                    <source  src="{{asset('video/Mock-up.m4v')}}" type="video/m4v" />Your browser does not support the video tag. I suggest you upgrade your browser.
                                    <source  src="{{asset('video/Mock-up.webm')}}" type="video/webm" />Your browser does not support the video tag. I suggest you upgrade your browser.
                                    <source  src="{{asset('video/Mock-up.ogg')}}" type="video/ogg" />Your browser does not support the video tag. I suggest you upgrade your browser.
                                    <source  src="{{asset('video/Mock-up.ogv')}}" type="video/ogv" />Your browser does not support the video tag. I suggest you upgrade your browser.
                                </video>
                                <div class="poster hidden">
                                    <img class="imgSlider" style="height:100%!important" src="{{ asset('img/slidehome.png')}}" alt="rendimento mensal garantido">
                                </div>
                            </div>
                        </div>
                        <div class="carousel-caption container">
                            <h1 class="title-slider">{% trans %}presence{% endtrans %}<br>{% trans %}internet{% endtrans %} <span class="textUnderlineBigSize">{% trans %}here{% endtrans %}.</span></h1>
                            <hr class="textUnderline ">
                            <h2 class="desc-slider"><p></p></h2>
                            <a class="page-scroll" href="#firstSection"><img src="{{ asset('img/setaBottom.png')}}" class="setaBottom" alt="rendimento mensal garantido"></a>
                        </div>
                        
                    </div>                    
                </div>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <section>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h1 class="tituloCinza">{% trans %}registration{% endtrans %}</h1>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <h1 class="tituloBold">{% trans %}registration1{% endtrans %}</h1>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <p>{% trans %}registration2{% endtrans %}
                <br><br>
                {% trans %}registration3{% endtrans %}
                <br><br>
                {% trans %}registration4{% endtrans %}
                <br><br>
                {% trans %}registration5{% endtrans %}
                <br><br>{% trans %}registration6{% endtrans %}<br><br>
                <a href="http://intouchbiz.com/pt/contactos-rua-municipio-domingos-lagoa-portimao-algarve-portugal" style="color:#21409A">{% trans %}contact_us{% endtrans %}</a>
                <br><br>
                {% trans %}registration7{% endtrans %}</p>               
            </div>
        </div>
    </section>
</div>

<section>
</section>


<div class="row">
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
</div>

<a href="{{ asset('img/setaService.png')}}">
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 botaoVerde hvr-sweep-to-right">
        <h1>{% trans %}more{% endtrans %}</h1>
    </div>
</a>

<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
</div>
</div>

