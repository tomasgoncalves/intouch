    //margin-top + servicos
    var height = $(window).height();
    function resizeContent() {
        var modal = $("#modal-bodyMaisServicos .container").height();
        var heightContent = ((modal) + 40);

        if (heightContent < height) {
            var heightModal = ((height - heightContent) * 0.5);

            //var heightModal = '100px';
            $("#modal-bodyMaisServicos .container").css({'margin-top': heightModal});

        } else {

            $("#modal-bodyMaisServicos .container").css({'margin-top': 0});

            $("body, html").css('overflow', 'hidden');

            $('.closeMaisServicos').click(function () {
                $("body, html").css('overflow', 'auto');
                });

                $(document).on('keyup', function (evt) {
                    if (evt.keyCode == 27) {
                        $("body, html").css('overflow', 'auto');
                    }
            });
        }
        $("body").css({'overflow': 'hidden'});
    };

