    }

    //PARTILHAR PRODUTO NO FACEBOOK
    function streamPublish(name, description, hrefTitle, hrefLink, userPrompt, image) {
        FB.ui({method: 'feed',
            message: userPrompt,
            picture: image,
            link: hrefLink,
            caption: hrefTitle
        });
    }

    function publishStream(idproduto) {
        streamPublish("Stream Publish", 'descrição', 'titulo href', 'http://www.intouchbiz.com/facebookPublish.php?idproduto=' + idproduto, "title");
    }

    function showHeight(element, height) {
        $(".imgSlider").css('height', height);
    }

    function resizeimage() {

        var height = $(window).height();
        var width = $(window).width();

        var menuScroll = $(".menu-scroll").height();

        //CALCULA A ALTURA DO SLIDER

        if (width >= height) {

            $(".imgSlider").css('width', width)
            height = height + 5;
            $(".imgSlider").css('height', height);

        } else if (width < height) {

            $(".imgSlider").css('width', 'auto');
            height = height + 5;
            $(".imgSlider").css('height', height);

        } else {

            $('html').text('oláaaaaaaaaaaaaaaaa');
            $(".imgSlider").css('width', 'auto');
            height = height + 5;
            $(".imgSlider").css('height', height);
        }

        //CALCULA O TAMANHO DO TITULO DO SLIDER E CENTRA

        var size = (height * width) / 150000;
        var min_size = 4;
        var max_size = 6.7;

        size = size < min_size ? min_size : size;

        size = size > max_size ? max_size : size;


        var alturaDiv = $(".carousel-caption").height();
        var alturaP = $(".title-slider").height();
        var alturaSeta = $(".setaBottom").height();

        $(".title-slider").css('margin-top', ((alturaDiv - alturaSeta) * .5) - (alturaP * 0.5));

        $(".title-slider").css('font-size', size + 'em');

        //CALCULA A ALTURA DOS SERVIÇOS NA HOME

        if (width <= 767) {

            //$(".servicosHome").css('height', height);
            $(".servicosHomeHover").css('background', 'transparent');
            $(".servicosColorText").css('color', 'rgb(255,255,255)');
        }

        //CALCULA A ALTURA DA DIV .parallaxHome

        $(".parallaxHome").css('height', (width * 500) / 2114);


        // scroll para baixo do banner

        $('.page-scroll').click(function () {

            $('html, body').stop().animate({
                scrollTop: $('#firstSection').offset().top - menuScroll
                }, 400);
                return false;
            });

            // scroll para baixo do funcionalidades

            $('.funcionalidades').click(function () {

                $('html, body').stop().animate({
                    scrollTop: $('#funcionalidades').offset().top - menuScroll
                    }, 400);
                return false;
            });

            // scroll para baixo do comparativo

            $('.comparativo').click(function () {

                $('html, body').stop().animate({
                scrollTop: $('#comparativo').offset().top - menuScroll
                }, 400);
            return false;
        });

    }

    //BACKGROUND DO MENU
    function backgroundMenu() {

        var nummenu = 1;

        if ($(window).scrollTop() > nummenu) {

            $('#header').addClass('menu-scroll');
